<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="lailyfaFebrinaCardVM" type="com.application.app.modules.lailyfafebrinacard.data.viewmodel.LailyfaFebrinaCardVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_45pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_26pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_left_icon_3"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtLailyfaFebrina"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_148pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="1" android:layout_width="0dp"
                    android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtLailyfaFebrina}"
                    style="@style/txtPoppinsbold16" tools:text="@string/msg_lailyfa_febrina"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_50pxv"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="match_parent" style="@style/groupStylelight_blue_A200cornerRadius">
                    <LinearLayout android:id="@+id/linear3"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginEnd="@dimen/_21pxh"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <ImageView android:id="@+id/image1"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_22pxv"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_credit_card_logo"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txt63269124"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_31pxv"
                            android:layout_width="match_parent"
                            android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txt63269124}"
                            style="@style/txtPoppinsbold24" tools:text="@string/msg_6326_9124"/>
                        <LinearLayout android:id="@+id/linear4"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtCardHolder"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtCardHolder}"
                                style="@style/txtPoppinsregular10_2" tools:text="@string/lbl_card_holder"/>
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtCardSave"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_132pxh"
                                android:layout_marginStart="@dimen/_37pxh"
                                android:layout_weight="0.4"
                                android:layout_width="0dp"
                                android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtCardSave}"
                                style="@style/txtPoppinsregular10_2" tools:text="@string/lbl_card_save"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linear5"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:gravity="top|center"
                                android:id="@+id/txtLailyfaFebrina1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.6"
                                android:layout_width="0dp"
                                android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtLailyfaFebrina1}"
                                style="@style/txtPoppinsbold10_3" tools:text="@string/lbl_lailyfa_febrina"/>
                            <TextView android:gravity="top|center"
                                android:id="@+id/txt192042"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_148pxh"
                                android:layout_marginStart="@dimen/_27pxh"
                                android:layout_weight="0.3"
                                android:layout_width="0dp"
                                android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txt192042}"
                                style="@style/txtPoppinsbold10_3" tools:text="@string/lbl_19_2042"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:id="@+id/linear6"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtCardNumber"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtCardNumber}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_card_number"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_border_blue_50_radius_5"
                        android:hint="@string/msg_1231_2312_3"
                        android:id="@+id/et123123123"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.et123123123Value}"
                        android:textColorHint="@color/bluegray_300" style="@style/etSolidRoundedOutline_1"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear7"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtExpirationDate"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtExpirationDate}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_expiration_date"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_border_blue_50_radius_5"
                        android:hint="@string/lbl_12_12"
                        android:id="@+id/et1212"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.et1212Value}"
                        android:textColorHint="@color/bluegray_300" style="@style/etSolidRoundedOutline_1"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear8"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtSecurityCode"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtSecurityCode}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_security_code"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_border_blue_50_radius_5"
                        android:hint="@string/lbl_1219"
                        android:id="@+id/et1219"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.et1219Value}"
                        android:textColorHint="@color/bluegray_300" style="@style/etSolidRoundedOutline_1"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear9"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtCardHolder3"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.txtCardHolder3}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_card_holder3"/>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_border_blue_50_radius_5"
                        android:hint="@string/lbl_lailyfa_febrina"
                        android:id="@+id/etLailyfaFebrina"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={lailyfaFebrinaCardVM.lailyfaFebrinaCardModel.etLailyfaFebrinaValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etSolidRoundedOutline_1"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:clickable="true" android:gravity="center"
                    android:id="@+id/btnSave"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_save"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_save"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
