<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addAddressVM" type="com.application.app.modules.addaddress.data.viewmodel.AddAddressVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_left_icon"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtAddAddress"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_213pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="0.6"
                    android:layout_width="0dp"
                    android:text="@{addAddressVM.addAddressModel.txtAddAddress}"
                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_add_address"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_50pxv"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linear3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtCountryOrRegi"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtCountryOrRegi}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/msg_country_or_regi"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_count"
                        android:id="@+id/etEnterTheCount"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheCountValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear4"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtFirstName"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtFirstName}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_first_name"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_first"
                        android:id="@+id/etEnterTheFirst"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheFirstValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear5"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtLastName"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtLastName}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_last_name"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_last"
                        android:id="@+id/etEnterTheLast"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheLastValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear6"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtStreetAddress"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtStreetAddress}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_street_address"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_stree"
                        android:id="@+id/etEnterTheStree"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheStreeValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear7"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtStreetAddress1"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtStreetAddress1}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/msg_street_address"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_stree2"
                        android:id="@+id/etEnterTheStree2"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheStree2Value}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear8"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtCity"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtCity}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_city"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/lbl_enter_the_city"
                        android:id="@+id/etEnterTheCity"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheCityValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear9"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtStateProvince"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtStateProvince}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/msg_state_province"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_state"
                        android:id="@+id/etEnterTheState"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheStateValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear10"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtZipCode"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtZipCode}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_zip_code"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_zip_c"
                        android:id="@+id/etEnterTheZipC"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterTheZipCValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear11"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtPhoneNumber"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="@{addAddressVM.addAddressModel.txtPhoneNumber}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_phone_number"/>
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_6"
                        android:hint="@string/msg_enter_the_phone"
                        android:id="@+id/etEnterThePhone"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@={addAddressVM.addAddressModel.etEnterThePhoneValue}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:gravity="center"
                    android:id="@+id/btnAddAddress"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_add_address"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_add_address"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
