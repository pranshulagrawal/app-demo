<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dashboardVM" type="com.application.app.modules.dashboard.data.viewmodel.DashboardVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linear2"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="1.3"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/image"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_16pxv"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_search_icon"/>
                    <TextView android:clickable="true"
                        android:gravity="top|start"
                        android:id="@+id/txtSearchProduct"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.8"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtSearchProduct}"
                        style="@style/txtPoppinsregular12" tools:text="@string/lbl_search_product"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_weight="0.4" android:layout_width="0dp">
                    <LinearLayout android:gravity="center"
                        android:id="@+id/linear3"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:clickable="true"
                            android:id="@+id/image1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_24pxv"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_love_icon"/>
                        <ImageView android:clickable="true"
                            android:id="@+id/image2"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_24pxv"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_notification_icon"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/image3"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_8pxv"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_notification_indicatore"/>
                </FrameLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollView"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linear4"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginTop="@dimen/_43pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frame1"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh" android:layout_width="match_parent">
                    <ImageView android:id="@+id/image4"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_206pxv"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        cornerRadius="@{@dimen/_5pxh}"
                        imageRes="@{@drawable/img_promotion_image_1}" tools:src="@drawable/img_promotion_image_1"/>
                    <LinearLayout android:id="@+id/linear5"
                        android:layout_gravity="center|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_32pxv"
                        android:layout_marginEnd="@dimen/_110pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:gravity="center"
                            android:id="@+id/txtSuperFlashSal"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:text="@{dashboardVM.dashboardModel.txtSuperFlashSal}"
                            style="@style/txtPoppinsbold24" tools:text="@string/msg_super_flash_sal"/>
                        <LinearLayout android:gravity="center"
                            android:id="@+id/linear6"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_29pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <androidx.appcompat.widget.AppCompatButton
                                android:gravity="center"
                                android:id="@+id/btn08"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_41pxv"
                                android:layout_width="@dimen/_42pxh"
                                android:text="@string/lbl_08"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_1" tools:text="@string/lbl_08"/>
                            <TextView android:gravity="top|center"
                                android:id="@+id/txtLbl"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_weight="0.1"
                                android:layout_width="0dp"
                                android:text="@{dashboardVM.dashboardModel.txtLbl}"
                                style="@style/txtPoppinsbold14_1" tools:text="@string/lbl"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:gravity="center"
                                android:id="@+id/btn34"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_41pxv"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_width="@dimen/_42pxh"
                                android:text="@string/lbl_34"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_1" tools:text="@string/lbl_34"/>
                            <TextView android:gravity="center"
                                android:id="@+id/txtLbl1"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_weight="0.1"
                                android:layout_width="0dp"
                                android:text="@{dashboardVM.dashboardModel.txtLbl1}"
                                style="@style/txtPoppinsbold14_1" tools:text="@string/lbl"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:gravity="center"
                                android:id="@+id/btn52"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_41pxv"
                                android:layout_marginEnd="@dimen/_59pxh"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_width="@dimen/_42pxh"
                                android:text="@string/lbl_52"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_1" tools:text="@string/lbl_52"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <ImageView android:gravity="center"
                    android:id="@+id/image5"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_152pxh"
                    android:layout_marginStart="@dimen/_151pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_72pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_slideshow_indicator"/>
                <LinearLayout android:id="@+id/linear7"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtCategory"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtCategory}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_category"/>
                    <TextView android:clickable="true"
                        android:gravity="top|end"
                        android:id="@+id/txtMoreCategory"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_157pxh"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtMoreCategory}"
                        style="@style/txtPoppinsbold14_3" tools:text="@string/lbl_more_category"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerCategories"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_12pxh}" tools:itemCount="6" tools:listitem="@layout/row_dashboard1"/>
                <LinearLayout android:id="@+id/linear8"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtFlashSale"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtFlashSale}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_flash_sale"/>
                    <TextView android:gravity="top|end"
                        android:id="@+id/txtSeeMore"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_197pxh"
                        android:layout_weight="0.4"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtSeeMore}"
                        style="@style/txtPoppinsbold14_3" tools:text="@string/lbl_see_more"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerFlashSaleProduct"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_dashboard2"/>
                <LinearLayout android:id="@+id/linear9"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtMegaSale"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtMegaSale}"
                        style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_mega_sale"/>
                    <TextView android:gravity="top|end"
                        android:id="@+id/txtSeeMore1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_195pxh"
                        android:layout_weight="0.4"
                        android:layout_width="0dp"
                        android:text="@{dashboardVM.dashboardModel.txtSeeMore1}"
                        style="@style/txtPoppinsbold14_3" tools:text="@string/lbl_see_more"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerMegaSaleProduct"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_dashboard3"/>
                <ImageView android:id="@+id/image6"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_206pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent"
                    imageRes="@{@drawable/img_recomended_product_banner}" tools:src="@drawable/img_recomended_product_banner"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
                    app:spanCount="2" itemSpace="@{@dimen/_13pxh}"
                    tools:itemCount="4" tools:listitem="@layout/row_dashboard4"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
        <FrameLayout android:id="@+id/frame2"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv" android:layout_width="match_parent">
            <View android:background="@color/white_A700"
                android:id="@+id/view" android:layout_gravity="center"
                android:layout_height="@dimen/_66pxv" android:layout_width="@dimen/_375pxh"/>
            <LinearLayout android:id="@+id/linear10"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linear11"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:gravity="center"
                        android:id="@+id/image7"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_home_icon"/>
                    <TextView android:gravity="center"
                        android:id="@+id/txtHome"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:text="@{dashboardVM.dashboardModel.txtHome}"
                        style="@style/txtPoppinsbold10_1" tools:text="@string/lbl_home"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear12"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:gravity="center"
                        android:id="@+id/image8"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_explore_icon"/>
                    <TextView android:gravity="center"
                        android:id="@+id/txtExplore"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:text="@{dashboardVM.dashboardModel.txtExplore}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_explore"/>
                </LinearLayout>
                <LinearLayout android:gravity="center"
                    android:id="@+id/linear13"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:gravity="center"
                        android:id="@+id/image9"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_cart_icon"/>
                    <TextView android:gravity="center"
                        android:id="@+id/txtCart"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:text="@{dashboardVM.dashboardModel.txtCart}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_cart"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear14"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:gravity="center"
                        android:id="@+id/image10"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_offer_icon"/>
                    <TextView android:gravity="center"
                        android:id="@+id/txtOffer"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:text="@{dashboardVM.dashboardModel.txtOffer}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_offer"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linear15"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:gravity="center"
                        android:id="@+id/image11"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_user_icon"/>
                    <TextView android:gravity="center"
                        android:id="@+id/txtAccount"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:text="@{dashboardVM.dashboardModel.txtAccount}"
                        style="@style/txtPoppinsregular10" tools:text="@string/lbl_account"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
