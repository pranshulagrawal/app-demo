<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="orderDetails1RowModel" type="com.application.app.modules.orderdetails.data.model.OrderDetails1RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frame"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700cornerRadius">
        <LinearLayout android:id="@+id/linear"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/image"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_72pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_72pxh"
                android:scaleType="centerCrop"
                cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_image_product}" tools:src="@drawable/img_image_product"/>
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_weight="1.4" android:layout_width="0dp" android:orientation="vertical">
                <LinearLayout android:gravity="center"
                    android:id="@+id/linear2"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtNikeAirZoomP"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.1"
                        android:layout_width="0dp"
                        android:text="@{orderDetails1RowModel.txtNikeAirZoomP}"
                        style="@style/txtPoppinsbold12_2" tools:text="@string/msg_nike_air_zoom_p"/>
                    <ImageView android:id="@+id/image1"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_love_icon_1"/>
                    <ImageView android:id="@+id/image2"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_trash_icon"/>
                </LinearLayout>
                <LinearLayout android:gravity="center"
                    android:id="@+id/linear3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txt29943"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:text="@{orderDetails1RowModel.txt29943}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_299_43"/>
                    <LinearLayout android:id="@+id/linear4"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_29pxh"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/image3"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxv"
                            android:layout_width="@dimen/_33pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_remove_1"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:gravity="center"
                            android:id="@+id/btn1"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxv"
                            android:layout_width="@dimen/_41pxh"
                            android:text="@string/lbl_1"
                            android:textAllCaps="false"
                            style="@style/btnSolidOutline" tools:text="@string/lbl_1"/>
                        <ImageView android:id="@+id/image4"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxv"
                            android:layout_width="@dimen/_33pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_add_1"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
