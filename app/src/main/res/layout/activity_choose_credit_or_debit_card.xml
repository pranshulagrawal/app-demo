<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chooseCreditOrDebitCardVM" type="com.application.app.modules.choosecreditordebitcard.data.viewmodel.ChooseCreditOrDebitCardVM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_left_icon"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtChooseCard"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="0.9"
                    android:layout_width="0dp"
                    android:text="@{chooseCreditOrDebitCardVM.chooseCreditOrDebitCardModel.txtChooseCard}"
                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_choose_card"/>
                <ImageView android:id="@+id/image1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_171pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_plus_icon_1"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_50pxv"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="match_parent" style="@style/groupStylelight_blue_A200cornerRadius">
                    <LinearLayout android:id="@+id/linear3"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_21pxh"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <ImageView android:id="@+id/image2"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_22pxv"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_credit_card_logo"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txt63269124"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_31pxv"
                            android:layout_width="match_parent"
                            android:text="@{chooseCreditOrDebitCardVM.chooseCreditOrDebitCardModel.txt63269124}"
                            style="@style/txtPoppinsbold24" tools:text="@string/msg_6326_9124"/>
                        <LinearLayout android:id="@+id/linear4"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtCardHolder"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:text="@{chooseCreditOrDebitCardVM.chooseCreditOrDebitCardModel.txtCardHolder}"
                                style="@style/txtPoppinsregular10_2" tools:text="@string/lbl_card_holder"/>
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtCardSave"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_132pxh"
                                android:layout_marginStart="@dimen/_37pxh"
                                android:layout_weight="0.4"
                                android:layout_width="0dp"
                                android:text="@{chooseCreditOrDebitCardVM.chooseCreditOrDebitCardModel.txtCardSave}"
                                style="@style/txtPoppinsregular10_2" tools:text="@string/lbl_card_save"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linear5"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:gravity="top|center"
                                android:id="@+id/txtDominicOvo"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:text="@{chooseCreditOrDebitCardVM.chooseCreditOrDebitCardModel.txtDominicOvo}"
                                style="@style/txtPoppinsbold10_3" tools:text="@string/lbl_dominic_ovo"/>
                            <TextView android:gravity="top|center"
                                android:id="@+id/txt192042"
                                android:layout_gravity="center_vertical"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_148pxh"
                                android:layout_marginStart="@dimen/_39pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.3"
                                android:layout_width="0dp"
                                android:text="@{chooseCreditOrDebitCardVM.chooseCreditOrDebitCardModel.txt192042}"
                                style="@style/txtPoppinsbold10_3" tools:text="@string/lbl_19_2042"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <ImageView android:gravity="center"
                    android:id="@+id/image3"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_151pxh"
                    android:layout_marginStart="@dimen/_152pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_72pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_slideshow_indicator"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:clickable="true" android:gravity="center"
                    android:id="@+id/btnPay76686"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_353pxv"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_pay_766_86"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_pay_766_86"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
